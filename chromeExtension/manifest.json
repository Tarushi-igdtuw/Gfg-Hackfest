{
    "manifest_version": 3,
    "name": "Mood Tracker",
    "version": "1.0",
    "permissions": 
    ["notifications",
     "storage",
     "tabs",          
     "activeTab" ,  
     "alarms"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    },
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
  }
  